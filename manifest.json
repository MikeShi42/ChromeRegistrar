{
    "manifest_version": 2,

    "name": "Chrome Registrar",
    "description": "Find the Registrar of any website.",
    "version": "0.0.1",

    "permissions": [
        "tabs",
        "webNavigation",
        "https://whoisology.com/",
        "https://plus.google.com/"
    ],

    "web_accessible_resources": [
        "effective_tld_names.dat"
    ],

    "background": {
        "scripts": [
            "jquery-2.1.1.min.js",
            "punycode.js",
            "publicsuffixlist.js",
            "listener.js"
        ]
    },
    "page_action": {
    }
}